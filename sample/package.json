{
  "name": "sample",
  "version": "1.0.1",
  "description": "Sample module using the ui-harness.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "start": "node .",
    "watch": "nodemon . -e js --ignore src",
    "lint": "sh ./node_modules/ui-harness.common/sh/lint.sh",
    "build": "sh ./node_modules/ui-harness.common/sh/build.sh",
    "reset": "rm -rf ./lib && rm -rf ./build && yarn build",
    "prepublish": "yarn run lint && yarn run build"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "ui-harness": "4.0.0-alpha.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philcockfield/ui-harness"
  },
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@types/react": "^15.6.0",
    "@types/react-dom": "^15.5.1"
  }
}
