{
  "name": "ui-harness",
  "version": "4.0.0-alpha.5",
  "description": "Isolate, test and document modular UI.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "test": "sh ./sh/test.sh",
    "lint": "node ./node_modules/tslint/bin/tslint 'src/**/*.ts{,x}' --format verbose --fix",
    "build": "rm -rf ./lib && node ./node_modules/typescript/bin/tsc",
    "prepublish": "yarn run lint && yarn test && yarn run build"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "glob": "^7.1.2",
    "minimist": "^1.2.0",
    "next": "^2.4.8",
    "rsync": "^0.6.1",
    "ui-harness.common": "^1.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.4",
    "@types/express": "^4.0.36",
    "@types/fs-extra-promise": "^1.0.2",
    "@types/glob": "^5.0.30",
    "@types/rsync": "^0.4.29"
  },
  "bin": {
    "ui-harness": "./bin/ui-harness"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philcockfield/ui-harness"
  },
